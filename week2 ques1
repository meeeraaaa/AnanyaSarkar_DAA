#include <bits/stdc++.h>
using namespace std;
class okay{
    public:

};
int main()
{
    int t,i,k,n,c=0,f,l,mid,x,y;
    int a[100];
    cout<<"test cases";
    cin>>t;
    okay dd;
    for(i=1;i<=t;i++)
    {
        c=0;
        cout<<"\n no. of elements";
        cin>>n;
        for(int j=1;j<=n;j++)
        {
            cout<<"\n element";
            cin>>a[j-1];
            
        }cin>>k;f=0;l=n-1;
        while(f<=l)
        {
            mid=(f+l)/2;
            if(a[mid]==k)
            {
                c++;
                cout<<" present "<<mid;
                break;
            }
            else if(a[mid]>k)
            {
                l=mid-1;
            }
            else if(a[mid]<k)
            {
                f=mid+1; 
            }
        }
        if(c!=0)
        {
            for(x=mid+1;a[x]<=k;x++)
            {
                if(a[x]==k){c++;cout<<" at "<<x;}
            }
            for( y=mid-1;a[y]>=k;y--)
            {
                if(a[y]==k){c++;cout<<" at "<<y;}
            }
            cout<<" "<<c<<" times";
        }
        else if(c==0) cout<<k<<" not present ";
    }

}

