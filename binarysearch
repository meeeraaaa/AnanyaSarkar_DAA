#include <iostream>
using namespace std;
class okay{
    public:
int find(int k,int a[],int f,int l)
{
    int mid=(f+l)/2;
    if(a[mid]==k)return (mid);
    else if(a[mid]>k)
    {
        return find(k,a,f,mid-1);
    }
    else if(a[mid]<k)
    {
       return find(k,a,mid+1,l); 
    }
    else return -999;
}
};
int main()
{
    int t,i,k,n;
    int a[100];
    cout<<"test cases";
    cin>>t;
    okay dd;
    for(i=1;i<=t;i++)
    {cout<<"\n no. of elements";
        cin>>n;
        for(int j=1;j<=n;j++)
        {
            cout<<"\n element";
            cin>>a[j-1];
            
        }cin>>k;
            int f=dd.find(k,a,0,n-1);
            if(f!=-999)cout<<f;
            else cout<<"not present";
    }

}